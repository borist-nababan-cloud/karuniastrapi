{
  "kind": "collectionType",
  "collectionName": "spks",
  "info": {
    "singularName": "spk",
    "pluralName": "spks",
    "displayName": "SPK",
    "description": "Sales Order (Surat Pesanan Kendaraan)"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "spkNumber": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "ON PROGRESS",
        "FINISH"
      ],
      "default": "ON PROGRESS"
    },
    "isEditable": {
      "type": "boolean",
      "default": true
    },
    "customerName": {
      "type": "string",
      "required": true
    },
    "customerPhone": {
      "type": "string"
    },
    "customerEmail": {
      "type": "email"
    },
    "notes": {
      "type": "text"
    },
    "sales": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "spks_sales"
    },
    "branch": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::branch.branch"
    },
    "vehicleType": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::vehicle-type.vehicle-type"
    },
    "documents": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ]
    },
    "paymentProofs": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    }
  }
}
