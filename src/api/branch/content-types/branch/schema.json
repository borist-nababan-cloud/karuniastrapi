{
  "kind": "collectionType",
  "collectionName": "branches",
  "info": {
    "singularName": "branch",
    "pluralName": "branches",
    "displayName": "Branch",
    "description": "Dealership branch locations"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": "text"
    },
    "latitude": {
      "type": "decimal"
    },
    "longitude": {
      "type": "decimal"
    },
    "spks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::spk.spk",
      "mappedBy": "branch"
    },
    "attendances": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::attendance.attendance",
      "mappedBy": "branch"
    }
  }
}