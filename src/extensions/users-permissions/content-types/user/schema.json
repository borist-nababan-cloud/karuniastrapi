{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false,
      "default": "local"
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "required": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "configurable": false
    },
    "fullName": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    },
    "whatsapp": {
      "type": "string"
    },
    "role_custom": {
      "type": "enumeration",
      "enum": [
        "SALES",
        "SPV",
        "ADMIN"
      ]
    },
    "isApproved": {
      "type": "boolean",
      "default": false
    },
    "lastLocation": {
      "type": "json"
    },
    "spks_sales": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::spk.spk",
      "mappedBy": "sales"
    },
    "attendances": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::attendance.attendance",
      "mappedBy": "user"
    },
    "profile_picture": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "supervisor": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::supervisor.supervisor"
    }
  }
}
